{
  "group": [
    {
      "name": "application.validation",
      "type": "com.labijie.application.configuration.ValidationConfiguration"
    },
    {
      "name": "application.okhttp",
      "type": "com.labijie.application.configuration.OkHttpClientProperties"
    },
    {
      "name": "application.sms",
      "type": "com.labijie.application.configuration.SmsBaseSettings"
    },
    {
      "name": "application.sms.async",
      "type": "com.labijie.application.configuration.SmsAsyncSettings"
    },
    {
      "name": "infra.snowflake.jdbc",
      "type": "com.labijie.application.snowflake.JdbcSlotProviderProperties"
    }
  ],
  "properties": [
    {
      "name": "application.sms.send-rate-limit",
      "sourceType": "com.labijie.application.configuration.SmsBaseSettings",
      "type": "java.time.Duration",
      "defaultValue": "10s"
    },
    {
      "name": "application.sms.async.sink-enabled",
      "sourceType": "com.labijie.application.configuration.SmsAsyncSettings",
      "type": "java.lang.Boolean",
      "defaultValue": true
    },
    {
      "name": "application.sms.async.message-expiration",
      "type": "java.time.Duration",
      "sourceType": "com.labijie.application.configuration.SmsAsyncSettings",
      "defaultValue": "1m"
    },
    {
      "name": "application.okhttp.enabled",
      "type": "java.lang.Boolean",
      "defaultValue": true
    },
    {
      "name": "application.validation.regex",
      "type": "java.util.Map<java.lang.String, java.lang.String>",
      "sourceType": "com.labijie.application.configuration.ValidationConfiguration"
    },

    {
      "name": "application.okhttp.max-connections",
      "type": "java.lang.Integer",
      "sourceType": "com.labijie.application.configuration.OkHttpClientProperties",
      "defaultValue": 200
    },
    {
      "name": "application.okhttp.ssl-validation-disabled",
      "type": "java.lang.Boolean",
      "sourceType": "com.labijie.application.configuration.OkHttpClientProperties",
      "defaultValue": true
    },
    {
      "name": "application.okhttp.time-to-live",
      "type": "java.time.Duration",
      "sourceType": "com.labijie.application.configuration.OkHttpClientProperties",
      "defaultValue": "5m"
    },
    {
      "name": "application.okhttp.connect-timeout",
      "type": "java.time.Duration",
      "sourceType": "com.labijie.application.configuration.OkHttpClientProperties",
      "defaultValue": "2s"
    },
    {
      "name": "application.okhttp.read-timeout",
      "type": "java.time.Duration",
      "sourceType": "com.labijie.application.configuration.OkHttpClientProperties",
      "defaultValue": "5s"
    },
    {
      "name": "application.okhttp.write-timeout",
      "type": "java.time.Duration",
      "sourceType": "com.labijie.application.configuration.OkHttpClientProperties",
      "defaultValue": "5s"
    },
    {
      "name": "application.okhttp.follow-redirects",
      "type": "java.lang.Boolean",
      "sourceType": "com.labijie.application.configuration.OkHttpClientProperties",
      "defaultValue": true
    },

    {
      "name": "infra.snowflake.jdbc.instance-identity",
      "type": "com.labijie.application.snowflake.InstanceIdentity",
      "sourceType": "com.labijie.application.snowflake.JdbcSlotProviderProperties",
      "defaultValue": "IP"
    },
    {
      "name": "infra.snowflake.jdbc.timeout",
      "type": "java.time.Duration",
      "sourceType": "com.labijie.application.snowflake.JdbcSlotProviderProperties",
      "defaultValue": "3m"
    }
  ],
  "hints": [
    {
      "name": "infra.snowflake.provider",
      "values": [
        {
          "value": "jdbc"
        }
      ]
    },
    {
      "name": "application.validation.regex.keys",
      "values": [
        {
          "value": "user-name"
        },
        {
          "value": "phone-number"
        },
        {
          "value": "code"
        }
      ],
      "providers": [
        {
          "name": "any"
        }
      ]
    },
    {
      "name": "spring.cloud.stream.bindings.keys",
      "values": [
        {
          "value": "handleSms-out-0"
        },
        {
          "value": "handleSms-in-0"
        }
      ],
      "providers": [
        {
          "name": "any"
        }
      ]
    }

  ]
}