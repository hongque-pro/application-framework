      "values": [
        {
          "value": "none"
        },
        {
          "value": "memory"
        },
        {
          "value": "jdbc"
        }
      ]
    },
    {
      "name": "application.validation.regex.keys",
      "values": [
        {
          "value": "user-name"
        },
        {
          "value": "phone-number"
        },
        {
          "value": "code"
        }
      ],
      "providers": [
        {
          "name": "any"
        }
      ]
    },
    {
      "name": "application.open-api.client.algorithm",
      "values": [
        {
          "value": "sha256"
        },
        {
          "value": "md5"
        }
      ]
    }
  ]
}